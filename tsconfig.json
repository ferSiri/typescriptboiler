{
  "include": ["src/**/*"],
    "compilerOptions": {
    "target": "es2016",
    "jsx": "react",
    "module": "commonjs",
     "allowJs": true,
     "checkJs": true,
    "declaration": true,
    "sourceMap": true,
     "outDir": "./built",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
